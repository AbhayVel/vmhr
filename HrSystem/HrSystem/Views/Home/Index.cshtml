                         <h3>New Applicant:</h3> <p>@Model[i].Applications.ID</p>
                    }
               

                </div>
            </div>

            <div class="column">
                <div class="card">
                    <h3>Active Vacancies</h3>
                    <p>2</p>

                </div>
            </div>
        </div>
    </div>


</div>
