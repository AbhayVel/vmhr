@model List<User>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<table>
    <thead>
        <tr>
            <th>Id</th>
             <th>FName</th>
              <th>Email</th>
              <th>Age</th>
               <th>Messsage</th>
       </tr>

    </thead>

    <tbody>

        @foreach(var item in Model){
        <tr>
            <td>
               @item.Id
            </td>
            <td>
                @item.FName
            </td>
            <td>
               @item.Email
            </td>
            <td>
                @item.Age
            </td>
            <td>
                @item.Message
            </td>
            </tr>
        }
            

    </tbody>
</table>